{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "type": "object",
  "title": "Walker Robot Frame",
  "description": "A frame that contains the positions and speeds of the joints",
  "properties": {
    
    "positions": {
      "description": "the coordinate of the joints",
      "type": "array",
      "items": {
        "type": "array",
        "items": {
          "type": "number"
        },
        "minItems": 2,
        "maxItems": 2
      },
      "minItems": 5,
      "maxItems": 5
    },

    "angles": {
      "description": "the angles of the joints",
      "type": "array",
      "items": {
        "type": "number"
      },
      "minItems": 5,
      "maxItems": 5
    },

    "terrain": {
      "description": "the coordinates to draw the terrain",
      "type": "array",
      "items": {
        "type": "array",
        "items": {
          "type": "number"
        }
      }
    }
  },
  "required": ["positions", "angles", "terrain"]
}